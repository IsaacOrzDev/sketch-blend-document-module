generator db {
  provider = "go run github.com/steebchen/prisma-client-go"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Document {
  id          String   @id @default(auto()) @map("_id") @db.ObjectId
  title       String
  description String?
  image       String?
  svg         String?
  paths       Json?
  createdAt   DateTime @db.Date
  updatedAt   DateTime @db.Date
}
